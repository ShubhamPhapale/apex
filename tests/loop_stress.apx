// Stress test: Deep loop nesting and complex ranges
// Expected: 60
fn main() -> i32 {
    let mut sum: i32 = 0;
    
    // Triple nested loops
    for i in 0..3 {
        for j in 0..3 {
            for k in 0..3 {
                sum = sum + 1;  // Should run 27 times
            }
        }
    }
    
    // Range with offsets in nested context
    let mut product: i32 = 1;
    for i in 2..5 {
        for j in 1..3 {
            product = product + i * j;  // (2*1 + 2*2) + (3*1 + 3*2) + (4*1 + 4*2) = 6 + 9 + 12 = 27
        }
    }
    
    // While with condition
    let mut count: i32 = 0;
    let mut val: i32 = 100;
    while val > 50 {
        count = count + 1;
        val = val - 10;  // Runs 5 times
    }
    
    return sum + product + count;  // 27 + 28 + 5 = 60
}
