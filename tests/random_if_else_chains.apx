// Random test: If-else chains with complex nesting
// Expected: 167
fn classify_number(n: i32) -> i32 {
    if n < 0 {
        return 1;
    } else {
        if n < 10 {
            return 2;
        } else {
            if n < 100 {
                return 3;
            } else {
                return 4;
            }
        }
    }
}

fn main() -> i32 {
    let mut result: i32 = 0;
    
    // Deep if-else nesting
    let x: i32 = 25;
    if x < 10 {
        result = result + 5;
    } else {
        if x < 20 {
            result = result + 10;
        } else {
            if x < 30 {
                result = result + 15;  // This branch
            } else {
                result = result + 20;
            }
        }
    }
    // result = 15
    
    // If-else in loops
    for i in 0..10 {
        if i < 3 {
            result = result + 2;  // 6
        } else {
            if i < 7 {
                result = result + 3;  // 12
            } else {
                result = result + 4;  // 12
            }
        }
    }
    // result = 15 + 30 = 45
    
    // Multiple if-else branches
    let y: i32 = 50;
    if y < 20 {
        result = result + 1;
    } else {
        if y < 40 {
            result = result + 2;
        } else {
            if y < 60 {
                result = result + 3;  // This
            } else {
                if y < 80 {
                    result = result + 4;
                } else {
                    result = result + 5;
                }
            }
        }
    }
    // result = 48
    
    // If-else with function calls
    let class1: i32 = classify_number(5);    // 2
    let class2: i32 = classify_number(50);   // 3
    let class3: i32 = classify_number(150);  // 4
    result = result + class1 + class2 + class3;  // 48 + 9 = 57
    
    // Nested if-else with arithmetic
    let z: i32 = 12;
    if z > 5 {
        if z > 10 {
            if z > 15 {
                result = result + 10;
            } else {
                result = result + 20;  // This
            }
        } else {
            result = result + 30;
        }
    } else {
        result = result + 40;
    }
    // result = 77
    
    // If-else expression
    let bonus: i32 = if result > 50 {
        if result > 75 {
            30
        } else {
            20
        }
    } else {
        10
    };  // 30
    
    result = result + bonus + 60;  // 77 + 30 + 60 = 167
    
    return result;
}
