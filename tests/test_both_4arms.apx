fn classify(n: i32) -> i32 {
    match n {
        0 => 100,
        1 => 110,
        2 => 120,
        _ => 999,
    }
}

fn main() -> i32 {
    let mut sum: i32 = 0;
    
    for i in 0..4 {
        let result: i32 = match i {
            0 => 10,
            1 => 20,
            2 => 30,
            _ => 40,
        };
        sum = sum + result;
    }
    
    let x: i32 = classify(1);
    
    return sum + x;  // Should be 100 + 110 = 210
}
