fn main() -> i32 {
    let mut sum: i32 = 0;
    
    let x: i32 = 15;
    if x < 5 {
        sum = sum + 1;
    } else {
        if x < 10 {
            sum = sum + 2;
        } else {
            if x < 20 {
                sum = sum + 3;
            } else {
                sum = sum + 4;
            }
        }
    }
    
    for i in 0..10 {
        if i < 5 {
            if i < 3 {
                sum = sum + 1;
            } else {
                sum = sum + 2;
            }
        } else {
            if i < 8 {
                sum = sum + 3;
            } else {
                sum = sum + 4;
            }
        }
    }
    
    return sum;
}
