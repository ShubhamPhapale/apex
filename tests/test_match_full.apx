fn classify(n: i32) -> i32 {
    match n {
        0 => 100,
        1 => 110,
        2 => 120,
        3 => 130,
        _ => 999,
    }
}

fn main() -> i32 {
    let mut sum: i32 = 0;
    
    for i in 0..5 {
        let result: i32 = match i {
            0 => 10,
            1 => 20,
            2 => 30,
            3 => 40,
            _ => 50,
        };
        sum = sum + result;
    }
    
    let x: i32 = classify(1);
    
    let z: i32 = match 1 + 1 {
        0 => 0,
        1 => 1,
        2 => 5,
        _ => 10,
    };
    
    let block_result: i32 = {
        let temp: i32 = 0;
        match temp {
            0 => 0,
            1 => 200,
            2 => 300,
            _ => 400,
        }
    };
    
    return sum + x + z + block_result;
}
