fn main() -> i32 {
    let mut sum: i32 = 0;
    
    // Match in loop
    for i in 0..5 {
        let result: i32 = match i {
            0 => 10,
            1 => 20,
            2 => 30,
            3 => 40,
            _ => 50,
        };
        sum = sum + result;
    }
    // Expected: 10 + 20 + 30 + 40 + 50 = 150
    return sum;
}
